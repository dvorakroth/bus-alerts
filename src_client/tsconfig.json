{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
      "outDir": "../dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es6",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "importsNotUsedAsValues": "remove",
      "noUnusedParameters": false,
      "strictNullChecks": true,
      "noUncheckedIndexedAccess": true
    }
  }